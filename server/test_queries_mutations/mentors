query {
   getMentors(fullnameSearch: "something") {
    status
    user {
      id
      email
      password
      fullname
      campus
      mentor
      location
      role
      current_job
      avatar
      getPrograms(input: {
        name: "UX"
      }) {
        id
        name
      }
    }
   }
 }



